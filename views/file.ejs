<div class="container-fluid mt-5">
  <table class="table table-hover table-bordered">
    <thead>
      <tr>
        <% for(let header of headers){ %>

        <th><%= header %></th>

        <% } %>
      </tr>
    </thead>

    <tbody>
      <% for(let record of records){ %>

      <tr>
        <% for(let data of record){ %>

        <td><%= data %></td>

        <% } %>
      </tr>

      <% } %>
    </tbody>
  </table>
</div>
